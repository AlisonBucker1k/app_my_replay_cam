{
  "expo": {
    "name": "MyReplayCam",
    "slug": "MyReplayCam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#272B30"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.alisonbucker1k.MyReplayCam",
      "buildNumber":"4",
      "infoPlist": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#272B30"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.alisonbucker1k.MyReplayCam"
    },
    "web": {
      "favicon": "./assets/app-icon.png"
    },
    "plugins": [
      "expo-video",
      "expo-media-library"
    ],
    "extra": {
      "eas": {
        "projectId": "43681a93-80c1-4bfa-b196-7a2bd65435cd"
      }
    }
  }
}
